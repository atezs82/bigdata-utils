<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.zenworks.infinispan.inquisitor.InfinispanWindowController">
<top>
  <ToolBar prefWidth="200.0" BorderPane.alignment="CENTER">
  <items><ComboBox fx:id="ipanHost" editable="true" prefWidth="150.0" /><Button fx:id="connectToIpan" mnemonicParsing="false" onAction="#onConnectToIpan" text="Connect to Infinispan">
<font>
<Font name="System Bold" size="12.0" />
</font></Button>
    <Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="0.0" /><ComboBox fx:id="ipanKey" editable="true" prefWidth="150.0" /><Button fx:id="queryButton" disable="true" mnemonicParsing="false" onAction="#onQueryKey" text="Query simple key">
<font>
<Font name="System Bold" size="12.0" />
</font></Button><Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="5.0" /><Button fx:id="refreshButton" disable="true" mnemonicParsing="false" onAction="#onRefreshStats" text="Refresh stats">
<font>
<Font name="System Bold" size="12.0" />
</font></Button>
  </items>
<BorderPane.margin>
<Insets />
</BorderPane.margin>
</ToolBar>
</top>
<center><TextArea fx:id="ipanContent" disable="true" editable="false" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
</center>
<bottom><BorderPane prefHeight="24.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
<left><Label fx:id="statLabel" text="Label" BorderPane.alignment="CENTER" />
</left></BorderPane>
</bottom></BorderPane>
